        $(".lunbo-tab li").on("click", function () {
            var tIndex=$(this).index();
            $("#loopDiv .main-box-r1").click()
            if(tIndex!=5){
                $("#starsIF img[alt="+tIndex+"]").click();
            }else {
                $($("#starsIF img[alt="+tIndex+"]")[0]).click();
            }
        })

        setTimeout(function () {
//            $($("#starsIF img")[0]).remove();
            $("#loopDiv .main-box-r1").on("click", function () {
                $($("#starsIF_images img").not(":hidden")[3]).click()
                $("#loopDiv .main-box-r2").css("z-index",18);
            })
            $("#loopDiv .main-box-l1").on("click", function () {
                $($("#starsIF_images img").not(":hidden")[1]).click()
                $("#loopDiv .main-box-r2").css("z-index",20);
            })

        },10)

//        图片放大
        setTimeout(function () {
            $(".img-box li").each(function () {
                var height=$(this).find("img")[0].offsetHeight;
                var width=$(this).find("img")[0].offsetWidth;

                $(this).height(height-26)
                $(this).width(width-10)
            })
            $(".img-box li").hover(function () {
                var ha=$(this).find("img")[0].offsetHeight;
                var wa=$(this).find("img")[0].offsetWidth;
                $(this).find("img").height(ha+30)
            }, function () {
                var hb=$(this).find("img")[0].offsetHeight;
                var wb=$(this).find("img")[0].offsetWidth;
                $(this).height(hb-56)
                $(this).width(wb-45)
                $(this).find("img").height(hb-30)
            })

        },2000)
        $(".content7 .img-box li").hover(function () {
            $(this).animate({"top":"10px","left":"-15px"},400)
        }, function () {
            $(this).animate({"top":"0px","left":"0px"},100)
        })

//
console.log(window.browser)

    })
    //   验证手机号
    function testPhone(value){
        var reg=/^(13[0-9]|14[5|7]|15[0|1|2|3|5|6|7|8|9]|18[0|1|2|3|5|6|7|8|9])\d{8}$/;
        return reg.test(value)
    }
    //   提示
    function tips(value,time,callback){
        $(".model-tips").show().text(value);
        setTimeout(function () {
            $(".model-tips").hide().text("");
        },time)
        if(callback){
            setTimeout(callback,time+1);
        }
    }
</script>
</body>
</html>
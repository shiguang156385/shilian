         if(!$("#user-phone").val()) {
                tips("联系方式不能为空",1000);

            }else if(!testPhone($("#user-phone").val())){
                tips("手机号格式不正确",1000)
            } else{
//                $(".close").click();
                tips("恭喜，领取成功",3000, function () {
                    $(".login-box").find("input").val("");
                })

            }

        })

        $(".course-info li").on("mouseenter", function () {
            var index=$(this).index();
            $($(".course-tab li")[index]).click();
//            $(".big-course-info").hide()
//            $(this).parent().parent().find(".big-course-info").show(50);
        })
        $(".course-tab li").on ("click", function () {
            $(this).addClass("selected").siblings().removeClass("selected");
            var index=$(this).index();
            $(".course-info li").find(".big-course-info").hide();
            $($(".course-info li")[index]).find(".big-course-info").show();

        })

